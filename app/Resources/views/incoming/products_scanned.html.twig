{% extends 'WarehouseBundle::layout.html.twig' %}

{% block title %}
    {{ parent() }} - Incoming Inventory Listing
{% endblock %}

{% block body %}

    <div class="page-title">
        <div class="title_left">
            <h3>{{ incoming.name }}'s Incoming Products <span class="glyphicon glyphicon-truck"
                                                              aria-hidden="true"></span></h3>
        </div>
        <div class="title_right"></div>
    </div>

    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12">
            {% for type, flashMessages in app.session.flashbag.all() %}
                {% for flashMessage in flashMessages %}
                    <div class="alert alert-{{ type }} alert-dismissible fade in">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
                                    aria-hidden="true">Ã—</span></button>
                        {{ flashMessage|raw }}
                    </div>
                {% endfor %}
            {% endfor %}
        </div>
    </div>

    {% include 'incoming/products_scanned_container_info.html.twig' with {'incoming':incoming} %}

    <!-- FORM -->
    <div id="scanned_form_wrap">
        {% include 'incoming/products_scanned_form.html.twig'
        with {'scannedProducts':scannedProducts, 'incoming':incoming,'locationDropdownList':locationDropdownList} %}
    </div>
    <!-- ENDFORM -->

    <div class="row">
        {% include 'incoming/products_scanned_packing_list.html.twig'
        with {'incoming':incoming} %}
    </div>

    <div class="dialabs hide">
        <div class="dialpad compact">
            <div class="number"></div>
            <div class="dials">
                <ol>
                    <li class="digits aisles"><p><strong>A</strong></p></li>
                    <li class="digits aisles"><p><strong>B</strong></p></li>
                    <li class="digits aisles"><p><strong>C</strong></p></li>
                    <li class="digits aisles"><p><strong>D</strong></p></li>
                    <li class="digits aisles"><p><strong>E</strong></p></li>
                    <li class="digits aisles"><p><strong>F</strong></p></li>
                    <li class="digits aisles"><p><strong>G</strong></p></li>
                    <li class="digits aisles"><p><strong>H</strong></p></li>
                    <li class="digits aisles"><p><strong>I</strong></p></li>
                    <li class="digits aisles"><p><strong>J</strong></p></li>
                    <li class="digits aisles"><p><strong>K</strong></p></li>
                    <li class="digits aisles"><p><strong>L</strong></p></li>
                    <li class="digits aisles pad-action" data-val="X - STAGING - 1"><p><strong><i class="fa fa-plus"></i></strong> <sup>STAGE</sup></p></li>
                    <li class="digits aisles"><p><strong>X</strong></p></li>
                    <li class="digits aisles pad-action" data-op="clear"><p><strong><i class="fa fa-refresh"></i></strong><sup>CLEAR</sup></p></li>

                    <li class="digits rows"><p><strong>1</strong></p></li>
                    <li class="digits rows"><p><strong>2</strong></p></li>
                    <li class="digits rows"><p><strong>3</strong></p></li>
                    <li class="digits rows"><p><strong>4</strong></p></li>
                    <li class="digits rows"><p><strong>5</strong></p></li>
                    <li class="digits rows"><p><strong>6</strong></p></li>
                    <li class="digits rows"><p><strong>7</strong></p></li>
                    <li class="digits rows"><p><strong>8</strong></p></li>
                    <li class="digits rows"><p><strong>9</strong></p></li>
                    <li class="digits rows"><p><strong>10</strong></p></li>
                    <li class="digits rows"><p><strong>11</strong></p></li>
                    <li class="digits rows"><p><strong>12</strong></p></li>
                    <li class="digits rows"><p><strong>13</strong></p></li>
                    <li class="digits rows"><p><strong>14</strong></p></li>
                    <li class="digits rows"><p><strong>15</strong></p></li>
                    <li class="digits rows"><p><strong>16</strong></p></li>
                    <li class="digits rows"><p><strong>17</strong></p></li>
                    <li class="digits rows"><p><strong>18</strong></p></li>
                    <li class="digits rows"><p><strong>19</strong></p></li>
                    <li class="digits rows"><p><strong>20</strong></p></li>
                    <li class="digits rows"><p><strong>21</strong></p></li>
                    <li class="digits rows"><p><strong>22</strong></p></li>
                    <li class="digits rows"><p><strong>23</strong></p></li>
                    <li class="digits rows"><p><strong>24</strong></p></li>
                    

                    <li class="digits levels"><p><strong>1</strong></p></li>
                    <li class="digits levels"><p><strong>2</strong></p></li>
                    <li class="digits levels"><p><strong>3</strong></p></li>
                    <li class="digits levels"><p><strong>4</strong></p></li>
                </ol>
            </div>
        </div>
    </div>

{% endblock %}



