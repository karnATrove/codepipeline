{% extends 'WarehouseBundle::layout.html.twig' %}

{% block title %}
{{ parent() }} - bookingReturn
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View bookingReturn <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ bookingReturn.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Created</strong></p>
        <p>
            {% if bookingReturn.created %}{{ bookingReturn.created|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Modified</strong></p>
        <p>
            {% if bookingReturn.modified %}{{ bookingReturn.modified|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Status</strong></p>
        <p>
            {{ bookingReturn.status }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('return') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('return_edit', { 'id':  bookingReturn.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('return_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('return_delete', { 'id': bookingReturn.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}

