<div class="col-xs-12">
    <div class="x_panel">
        <div class="x_title">
            <h2><i class="fa fa-bars"></i> Packing List
                <small>Assigned by client</small>
            </h2>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">

            <table id="datatable-keytable" class="table table-striped table-bordered">
                <thead>
                <tr>
                    <th>{{ 'Model' }}</th>
                    <th>{{ 'Qty Incoming' }}</th>
                    <th>{{ 'Total Qty Scanned' }}</th>
                </tr>
                </thead>
                <tbody class="incomingProducts">
                {% for incomingProduct in incoming.incomingProducts %}
                    {% if incomingProduct.qty==incomingUtil.getIncomingProductScanCountByModel(incoming,incomingProduct.product.model) %}
                        <tr>
                    {% else %}
                        <tr class="warning">
                    {% endif %}

                    <td>{{ incomingProduct.product.model }}</td>
                    <td>{{ incomingProduct.qty }}</td>
                    <td>{{ incomingUtil.getIncomingProductScanCountByModel(incoming,incomingProduct.product.model) }}</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>

            <div class="clearfix"></div>

        </div>
    </div>

</div>