<tr>
    <td><a href="{{ path('booking_edit',{'id':incomingProduct.incoming.id}) }}">{{ incomingProduct.incoming.name }}</a></td>
    <td>{{ incomingUtil.incomingTypeName(incomingProduct.incoming.type) }}</td>
    <td>{{ incomingProduct.qty }}</td>
    <td>{{ incomingUtil.getIncomingProductScanCountByModel(incomingProduct.incoming,incomingProduct.product.model) }}</td>
    <td>{{ incomingUtil.incomingStatusName(incomingProduct.incoming.status) }}</td>
    <td class="hidden-xs">{% if incomingProduct.incoming.eta %}{{ incomingProduct.incoming.eta|date('Y-m-d') }}{% else %}unknown{% endif %}</td>
    <td class="actions">
        <a  title='Edit' class="btn btn-primary btn-sm" href="{{ path('booking_product_edit', { 'id': incomingProduct.id }) }}">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        </a>
    </td>
</tr>