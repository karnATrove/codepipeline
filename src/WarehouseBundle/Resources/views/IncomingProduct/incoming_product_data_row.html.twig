<tr>
    <td><a href="{{ path('booking_edit',{'id':incomingProduct.incoming.id}) }}">{{ incomingProduct.incoming.name }}</a>
        <div class="visible-xs">
            {{ incomingProduct.incoming.type.detail }}
        </div>
    </td>
    <td class="hidden-xs">{#{ incomingUtil.incomingTypeName(incomingProduct.incoming.type) }#}{{ incomingProduct.incoming.type.detail }}</td>
    <td>{{ incomingProduct.qty }}</td>
    <td>{{ incomingUtil.getIncomingProductScanCountByModel(incomingProduct.incoming,incomingProduct.product.model) }}</td>
    <td>{#{ incomingUtil.incomingStatusName(incomingProduct.incoming.status) }#}{{ incomingProduct.incoming.status.detail }}</td>
    <td class="hidden-xs">{% if incomingProduct.incoming.eta %}{{ incomingProduct.incoming.eta|date('Y-m-d') }}{% else %}unknown{% endif %}</td>
    <td class="actions hidden-xs">
        <a  title='Edit' class="btn btn-primary btn-sm" href="{{ path('booking_product_edit', { 'id': incomingProduct.id }) }}">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        </a>
    </td>
</tr>