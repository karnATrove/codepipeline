{% extends 'WarehouseBundle::layout.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-xs-12">

            <div class="x_panel">
                <div class="x_title">
                    <h2><i class="fa fa-eye"></i> Picking/Staging Queue
                        <small>Live updates</small>
                    </h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                        <li><a class="close-link"><i class="fa fa-close"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                </div>

                <div class="x_content">
                    <div class="content_heading">Pick Queue - As of <span class="asof">{{ "now"|date('h:i:s A') }}</span></div>
                    {% block content %}
                        <div class="">
                            <div class="">
                                <h2></h2>
                                {{ form_start(form,{ attr: { id: "pickingStagingQueue", class:"ajaxForm", 'data-autorefresh':'on', 'data-autoselector':'#pickingStagingQueue', 'data-autourl':path('booking_pick_queue_refresh_ajax') }}) }}
                                    {% include '@warehouse/Booking/pick_queue_form.html.twig' with {'form': form} %}
                                {{ form_end(form) }}
                            </div>
                        </div>
                    {% endblock %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}